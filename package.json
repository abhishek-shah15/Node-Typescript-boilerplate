{
  "name": "node-typescript-api",
  "version": "1.0.0",
  "description": "Node Typescript Api",
  "main": "build/index.js",
  "scripts": {
    "watch": "env-cmd .env nodemon --watch package.json -x  npm run watch:serve",
    "watch:serve": "tsc && npm start",
    "start": "node --inspect -r ts-node/register src/index.ts",
    "dev": "env-cmd .env nodemon --inspect dist/index.js",
    "build:tsc": "tsc",
    "copy-files": "npm-run-all copy:*",
    "copy:serviceKey": "copyfiles -u 1 src/serviceAccountKey.json dist",
    "copy:package-files": "copyfiles package.json package-lock.json .npmrc befast.config.js dist",
    "lint": "tslint --type-check -c tslint.json -p tsconfig.json",
    "build": "npm-run-all build:tsc copy-files",
    "cleanup": "rm -rf dist && rm dist.zip",
    "package": "npm run build && zip -r dist.zip dist  && echo \"Package built: dist.zip\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/krypton-inc/kidzipp-api.git"
  },
  "dependencies": {
    "@types/express-fileupload": "^1.2.2",
    "@types/node": "^13.13.1",
    "aws-sdk": "2.141.0",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "3.5.1",
    "body-parser": "^1.18.2",
    "cors": "2.8.4",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.4.0",
    "firebase-admin": "^5.13.1",
    "helmet": "3.9.0",
    "inversify": "4.4.0",
    "inversify-express-utils": "4.0.1",
    "joi": "10.6.0",
    "joi-date-extensions": "^1.2.0",
    "jsonwebtoken": "^8.2.2",
    "mailgun-js": "^0.22.0",
    "mongoose": "^6.4.4",
    "mongoose-autopopulate": "^0.8.0",
    "morgan": "^1.9.1",
    "mustache": "^2.3.0",
    "nodemailer": "^4.6.7",
    "nodemailer-smtp-transport": "^2.7.4",
    "phantomjs-prebuilt": "^2.1.16",
    "pino": "4.8.1",
    "reflect-metadata": "^0.1.10",
    "request": "2.83.0",
    "socket.io": "^2.1.0",
    "twilio": "^3.77.2",
    "typescript": "^4.4.4",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "3.5.17",
    "@types/body-parser": "1.16.7",
    "@types/compression": "0.0.34",
    "@types/cors": "2.8.1",
    "@types/express": "4.0.39",
    "@types/faker": "4.1.1",
    "@types/helmet": "0.0.37",
    "@types/jest": "21.1.5",
    "@types/joi": "10.4.4",
    "@types/jsonwebtoken": "7.2.3",
    "@types/knex": "0.0.64",
    "@types/mailgun-js": "^0.22.12",
    "@types/mongoose": "^5.5.18",
    "@types/morgan": "1.7.35",
    "@types/mustache": "^0.8.30",
    "@types/nodemailer": "^4.6.0",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/pino": "^4.7.0",
    "@types/request": "2.0.7",
    "@types/socket.io": "^1.4.33",
    "@types/supertest": "2.0.3",
    "@types/twilio": "0.0.9",
    "@types/uuid": "3.4.3",
